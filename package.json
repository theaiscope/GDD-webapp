{
  "name": "gdd-webapp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install:client": "cd client && yarn",
    "install:server": "cd server && yarn",
    "install:all": "yarn && concurrently -n \"Client Install,Server Install\" -c \"blue,green\"  \"yarn install:client\" \"yarn install:server\"",
    "start:client": "cd client && yarn start",
    "start:server": "cd server && yarn start",
    "start:all": "concurrently -n \"Client Start,Server Start\" -c \"blue,green\"  \"yarn start:client\" \"yarn start:server\"",
    "lint:client": "cd client && yarn lint",
    "lint:server": "cd server && yarn lint",
    "lint:all": "concurrently -n \"Client Lint,Server Lint\" -c \"blue,green\"  \"yarn lint:client\" \"yarn lint:server\"",
    "test:client": "cd client && yarn test",
    "test:server": "cd server && yarn test",
    "test:all":  "yarn && concurrently -n \"Client Test,Server Test\" -c \"blue,green\"  \"yarn test:client\" \"yarn test:server\"",
    "prettier:client": "cd client && yarn prettier",
    "prettier:server": "cd server && yarn prettier",
    "prettier:all": "concurrently -n \"Client Prettier,Server Prettier\" -c \"blue,green\"  \"yarn prettier:client\" \"yarn prettier:server\"",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^6.2.0",
    "husky": "^7.0.4"
  }
}
